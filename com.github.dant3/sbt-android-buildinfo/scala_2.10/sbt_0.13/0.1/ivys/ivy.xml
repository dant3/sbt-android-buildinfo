<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:e="http://ant.apache.org/ivy/extra">
	<info organisation="com.github.dant3" module="sbt-android-buildinfo" revision="0.1" status="release" publication="20141030013726" e:scalaVersion="2.10" e:sbtVersion="0.13">
		<license name="Apache-2.0" url="https://www.apache.org/licenses/LICENSE-2.0.html"/>
		<description>
		sbt plugin to generate build info xml file
		</description>
	</info>
	<configurations>
		<conf name="compile" visibility="public" description=""/>
		<conf name="runtime" visibility="public" description="" extends="compile"/>
		<conf name="test" visibility="public" description="" extends="runtime"/>
		<conf name="provided" visibility="public" description=""/>
		<conf name="optional" visibility="public" description=""/>
		
		
		
		
		<conf name="sources" visibility="public" description=""/>
		<conf name="docs" visibility="public" description=""/>
		<conf name="pom" visibility="public" description=""/>
		
		
		
	</configurations>
	<publications>
		<artifact name="sbt-android-buildinfo" type="jar" ext="jar" conf="compile"/>
		<artifact name="sbt-android-buildinfo" type="src" ext="jar" conf="sources" e:classifier="sources"/>
		<artifact name="sbt-android-buildinfo" type="src" ext="jar" conf="sources" e:classifier="tests-sources"/>
		<artifact name="sbt-android-buildinfo" type="doc" ext="jar" conf="docs" e:classifier="javadoc"/>
		<artifact name="sbt-android-buildinfo" type="doc" ext="jar" conf="docs" e:classifier="tests-javadoc"/>
	</publications>
	<dependencies>
		
		<dependency org="org.scala-lang" name="scala-library" rev="2.10.4" conf="provided->default(compile)"/>
		<dependency org="org.scala-sbt" name="sbt" rev="0.13.5" conf="provided->default(compile)"/>
		
		
	</dependencies>
</ivy-module>
